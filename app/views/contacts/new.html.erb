<div class="container">
　<div class="row">
　　<%= form_with model:@contact, url: contacts_path, local:true do |f| %>
　　<h1>お問い合わせ</h1>
　　  <h4>お名前</h4>
　　  <%= f.text_field :name %>
　　  <h4>ご住所</h4>
　　  <%= f.select :address, Contact.addresses.keys, {} %>
　　  <h4>email</h4>
　　  <%= f.text_field :email %>
　　  <h4>お電話番号(ハイフンなし)</h4>
　　  <%= f.text_field :phone_number %>
　　  <h4>メッセージ</h4>
　　  <%= f.text_area :massage, size: "40x10" %>
　　  <%= f.submit "お問い合わせ送信", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
<div class="container">
　<div class="row">
　  <div class="col-xs-offset-1">
　  <%= form_with model:@schedule, url: directors_schedules_path, local:true do |f| %>
　    <h1>予定追加</h1>
　    <h4>予定日</h4>
　    <%= f.datetime_select :start_time %>
　    <h4>予定内容</h4>
　    <%= f.text_field :content %>
      <%= f.submit "予定追加", class: "btn btn-primary" %>
    </div>
　  <% end %>
　  <%= month_calendar events: @schedules do |date, schedules| %>
      <%= date.day %>
      <% schedules.each do |schedule| %>
      <%= link_to schedule.content, edit_directors_schedule_path(schedule) %>
      <% end %>
    <% end %>
　</div>
</div>
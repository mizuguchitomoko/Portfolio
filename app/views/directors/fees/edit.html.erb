<div class="container">
　<div class="row">
　  <h3>料金表編集</h3>
　  <%= form_with model:@fee, url:"/directors/fees/#{@fee.id}", local: true do |f| %>
　    <h4>コース内容</h4>
      <%= f.text_field :course %>
　    <h4>料金</h4>
      <%= f.text_field :price %>
      <%= f.submit "編集を保存", class: "btn btn-primary" %>
　  <% end %>
　</div>
</div>
<div class="container">
　<div class="row">
　  <div class="col-xs-offset-1">
     <h1>Schedule</h1>
  　  <%= month_calendar events: @schedules do |date, schedules| %>
        <%= date.day %>
        <% schedules.each do |schedule| %><br />
        <%= schedule.start_time.strftime('%H:%M') %>
        <%= link_to schedule.content,schedule_path(schedule) %>
        <% end %>
      <% end %>
　</div>
</div>